{
    "manifest_version": 3,
    "name": "Dictionary",
    "version": "1.0.1",
    "icons":{
        "128":"assets/dictionary.png"
    },
    "permissions":["clipboardRead","storage", "activeTab", "scripting"
    ,"tabs","storage","fontSettings","contentSettings","debugger"],
    "action":{
        "default_icon":"assets/dictionary.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }]
     
}